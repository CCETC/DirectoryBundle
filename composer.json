{
    "name": "ccetc/directory-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for building a web-based directory of producers.  It allows users to search/browse listings by category and location.",
    "keywords": ["symfony", "commerce", "directory"],
    "homepage": "http://github.com/CCETC/DirectoryBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Pat Haggerty",
            "email": "haggertypat@gmail.com",
            "homepage": "http://github.com/somewherethere"
        }
    ],
    "require": {
        "symfony/http-foundation": "~2.2",
        "symfony/form": "~2.2",
        "symfony/security": "~2.2",
        "symfony/console": "~2.2",
        "twig/twig": ">=1.10,<2.0-dev",
        "knplabs/knp-menu": ">=1.1.0,<3.0.0",
        "knplabs/knp-menu-bundle": ">=1.1.0,<3.0.0",
        "sonata-project/admin-bundle": "2.2.*@dev",
        "sonata-project/doctrine-orm-admin-bundle": "2.2.*@dev",
        "sonata-project/core-bundle": "~2.2@dev",
        "doctrine/common": ">=2.2,<3.0",
        "ccetc/directory-user-bundle": "dev-master",
        "friendsofsymfony/user-bundle": "~1.3",        
        "mopa/bootstrap-bundle": "~2.1"
    },
    "autoload": {
        "psr-0": { "CCETC\\DirectoryBundle": "" }
    },
    "target-dir": "CCETC/DirectoryBundle",
    "minimum-stability": "dev"
}
